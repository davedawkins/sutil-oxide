{
  "repository": {
    "url": "https://github.com/davedawkins/sutil-oxide.git"
  },
  "private": true,
  "scripts": {
    "postinstall": "dotnet tool restore",
    "start": "dotnet fable watch src/App --run webpack-dev-server",
    "build": "dotnet fable src/App && webpack",
    "clean": "dotnet fsi build.fsx --target clean",
    "pack": "dotnet fsi build.fsx --target pack",
    "publish": "node publish.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^0.28.8",
    "fable-publish-utils": "^2.2.0",
    "gh-pages": "^4.0.0",
    "style-loader": "^0.23.1",
    "toastr": "^2.1.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.2"
  },
  "version": "1.0.0"
}
